@model List<Student>;
<table id="mytable" class="table table-bordered table-striped mb-2">
    <thead>
        <tr>
            <th>Name</th>
            <th class ="w-20">Family</th>
            <th class="w-50">national-code</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
               <td>@item.Name</td>
               <td>@item.Family</td>
               <td>@item.National_code</td>
               <td>@item.Phone</td>
            </tr>
        }
    </tbody>

</table>
